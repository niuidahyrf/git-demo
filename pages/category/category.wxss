/* pages/category/category.wxss */

.cate{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;

}
.cate .cate-item{
  background:#fff;
  width:25%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.cate-item image{
  width: 110rpx;
  height: 110rpx;
  padding:20rpx 0;
}
.cate-item text{
  font-size:30rpx;
  padding-bottom:20rpx;
  color: #3963BC;
}

